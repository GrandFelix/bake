{
    "name": "company/example",
    "description": "Company/Example plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "cakephp/cakephp": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.19"
    },
    "autoload": {
        "psr-4": {
            "Company\\Example\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Company\\Example\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
